pipeline {
    agent any
    tools {
        jdk 'JDK9.0.4'
        maven 'maven3.5.3'
    }
    stages {

        stage('Checkout') {
            checkout scm
        }

        stage('Test') {
            executeTest()
        }

        stage('Devloy') {
            executeDeploy
        }
    }
}

def executeTest() {
    sh 'mvn clean test'
}

def executeDeploy() {
    sh 'mvn clean deploy'
}
